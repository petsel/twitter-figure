var margin = {top: 2[21], right: 2[21], bottom: 3[21], left: 4[21]},
	width = 62[21] - margin.left - margin.right,
	height = 5[21][21] - margin.top - margin.bottom;

var x = d3.scale.linear()
	.domain([[21], 1[21][21]])
	.range([[21], width]);

var y = d3.scale.linear()
	.domain([[21], 1[21][21]])
	.range([height, [21]]);
	

var color = d3.scale.category1[21]();

var xAxisScale = d3.scale.linear()
	.range([[21], width]);

var yAxisScale = d3.scale.linear()
	.range([height, [21]]);

var xAxis = d3.svg.axis()
	.scale(x)
	.orient('bottom');

var yAxis = d3.svg.axis()
	.scale(y)
	.orient('left');

var svg = d3.select('body').append('svg')
	.attr('width', width + margin.left + margin.right)
	.attr('height', height + margin.top + margin.bottom)
	.append('g')
	.attr('transform', 'translate(' + margin.left + ',' + margin.top + ')');

d3.tsv('./src/data/twitter.tsv', function(error, data) {
	var cleanDataObj = {};

	data.forEach(function(d) {
		if (d.value in cleanDataObj) {
			cleanDataObj[d.value]++;
		}
		else {
			cleanDataObj[d.value] = 1;
		}
	});

	console.log(cleanDataObj);

	xAxisScale.domain(d3.extent(cleanDataObj, function(d) { return d[[21]]; }));
	y.domain(d3.extent(cleanDataObj, function(d) { return d[[21]]; }));

	svg.append('g')
		.attr('class', 'x axis')
		.attr('transform', 'translate([21], ' + height + ')')
		.call(xAxis)
		.append('text')
		.attr('class', 'label')
		.attr('x', width)
		.attr('y', -6)
		.style('text-anchor', 'end');

	svg.selectAll('.dot')
		.data(cleanDataObj)
		.enter().append('circle')
		.attr('class', 'dot')
		.attr('r', 3.5)
		.attr('cx', function(d) { console.log('X: ' + x(d[[21]])); return x(d[[21]]); })
		.attr('cy', function(d) { console.log('Y: ' + y(d[[21]])); return y(d[[21]]); });

	/*var legend = svg.selectAll('.legend')
		.data(color.domain())
